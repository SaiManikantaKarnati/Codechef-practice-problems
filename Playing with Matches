# Playing with Matches

cases = int(input())
for i in range(cases):
    a,b = map(int,input().split())
    mydict = {'0':6,'1':2,'2':5,'3':5,'4':4,'5':5,'6':6,'7':3,'8':7,'9':6}
    total = a + b
    total = str(total)
    result = 0
    for i in total:
        result += mydict[i]
    print(result)
    
